@model IEnumerable<FootballManagerApp.Models.Coach_Team>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Team1.abbreviation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coach1.coach_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.wage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.expirationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.breakupDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Team1.abbreviation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Coach1.coach_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.wage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.expirationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.breakupDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { coach = item.coach, team = item.team, signDate = item.signDate }) |
            @Html.ActionLink("Details", "Details", new { coach = item.coach, team = item.team, signDate = item.signDate }) |
            @Html.ActionLink("Delete", "Delete", new { coach = item.coach, team = item.team, signDate = item.signDate })
        </td>
    </tr>
}

</table>
